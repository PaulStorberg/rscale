<div class="Admin-users-index" style="text-align: center;">

  <h5>All Users</h5>
  <input type="text" id="myInput" onkeyup="searchTable()" placeholder="Search for emails..." title="Type in a name">
  <div class="table-wrapper">
    <table id="myTable" class="fl-table">
      <thead>
        <tr>
          <th>Email</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>View</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.email %></td>
            <td><%= user.first_name %></td>
            <td><%= user.last_name %></td>
            <td><%= link_to('View', admin_user_path(user.id)) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

<%= line_chart User.group(:created_at).count %>
<%= pie_chart User.group(:created_at).count %>
<%= column_chart User.group_by_hour_of_day(:created_at, format: "%l %P").count %>

<script>
  searchTable();
</script>
